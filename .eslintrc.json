{
    "extends": "airbnb-base",
    "plugins": [
        "vue",
        "jest",
        "cypress"
    ],
    "rules": {
        "semi": ["error", "never"],
        "arrow-parens": ["error", "as-needed"],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    },
    "env": {
        "jest/globals": true,
        "cypress/globals": true
    }
}